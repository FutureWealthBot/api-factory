{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/_next/static/(.*)", "dest": "/_next/static/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "NEXT_PUBLIC_ENABLE_RETIREMENT_PLANNER": ""
  }
}
