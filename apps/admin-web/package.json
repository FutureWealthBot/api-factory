{
  "name": "admin-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
  "dev": "node ./scripts/wait-for-api.js && ADMIN_BASE_URL=http://localhost:5173 pnpm exec vite -- --port 5173",
  "start": "node ./scripts/wait-for-api.js && ADMIN_BASE_URL=http://localhost:5173 pnpm exec vite -- --port 5173",
  "dev:supervised": "pnpm --filter ./apps/admin-web dev",
  "test": "vitest",
  "test:ci": "vitest run",
  "test:playwright": "playwright test --project=chromium",
    "build": "tsc -p tsconfig.json && pnpm exec vite build",
    "preview": "pnpm exec vite preview -- --port 5173"
  },
  "dependencies": {
  "react": "^19.1.1",
  "react-dom": "^19.1.1",
    "react-router-dom": "^6.14.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
  "typescript": "^5.9.2",
  "vite": "^7.1.5",
  "vitest": "^3.2.4",
  "@testing-library/react": "^14.0.0",
  "@testing-library/jest-dom": "^6.0.0",
  "jsdom": "^22.1.0"
  }
  ,"turbo": { "pipeline": { "dev": { "dependsOn": [] } } }
}
