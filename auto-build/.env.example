# This file provides an example of environment variables required for the project.
# It serves as a template for creating a .env file.

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=username
DB_PASSWORD=password
DB_NAME=database_name

# Application settings
APP_ENV=development
APP_DEBUG=true
APP_PORT=3000

# API keys
API_KEY=your_api_key_here
SECRET_KEY=your_secret_key_here

# Other configurations
LOG_LEVEL=info
CACHE_ENABLED=true