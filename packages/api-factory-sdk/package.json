{
  "name": "@api-factory/sdk-ts",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "gen": "openapi-typescript-codegen --input openapi.yaml --output src",
  "build": "tsc -p tsconfig.json && node ./scripts/bundle.mjs",
  "prepublishOnly": "pnpm build",
  "test": "pnpm exec jest --config ./jest.config.cjs"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
  "openapi-typescript-codegen": "0.29.0"
  }
}