{
  "name": "api-factory",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.9.0",
  "version": "0.1.1",
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "api-factory/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "(cd api-factory/backend && pnpm test)",
    "build": "tsc -p tsconfig.json || echo \"no tsc\"",
    "dev": "turbo run dev",
    "dev:fortress": "node ./scripts/dev-supervisor.js",
    "dev:admin": "pnpm --filter admin-web dev",
    "start:admin": "pnpm --filter admin-web dev -- --host",
    "start": "next start"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime": "^1.3.5",
    "@types/node": "^24.3.1",
    "@vitejs/plugin-react": "^5.0.2",
    "concurrently": "^9.1.0",
    "esbuild": "^0.25.9",
    "jest": "29.0.0",
    "node-fetch": "^3.3.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/session": "11.1.0",
    "@fastify/static": "8.2.0",
    "@supabase/supabase-js": "^2.27.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "fastify": "^5.6.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.5.3",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "siwe": "^3",
    "supertest": "^7.1.4",
    "uuid": "^12.0.0"
  },
  "pnpm": {
    "overrides": {
      "fastify": "5.6.0"
    }
  },
  "description": "import React from \"react\"; import { createRoot } from \"react-dom/client\"; import { promises as fs } from \"node:fs\"; import path from \"node:path\"; import { createClient } from \"@supabase/supabase-js\"; import client from \"prom-client\";",
  "main": "eslint.config.js",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
